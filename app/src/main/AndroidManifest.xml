<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.mydentalcare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dentist"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dental"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".EstimateScore" />
        <activity android:name=".EstimateScore2" />
        <activity android:name=".Result" />
        <activity android:name=".About" />
        <activity android:name=".HospitalData"></activity>
    </application>

</manifest>